local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local UIS = game:GetService("UserInputService")

-- Remove old GUI
if LP:FindFirstChild("PlayerGui") then
    local oldGui = LP.PlayerGui:FindFirstChild("LoaderUI")
    if oldGui then oldGui:Destroy() end
end

getgenv()._LOADER_CLOSED = false
getgenv()._LOADER_LOADED = true

-- ScreenGui
local Gui = Instance.new("ScreenGui")
Gui.Name = "LoaderUI"
Gui.ResetOnSpawn = false
Gui.Parent = LP:WaitForChild("PlayerGui")

-- Main Frame
local Main = Instance.new("Frame")
Main.Size = UDim2.fromScale(0.45,0.5)
Main.Position = UDim2.fromScale(0.5,0.5)
Main.AnchorPoint = Vector2.new(0.5,0.5)
Main.BackgroundColor3 = Color3.fromRGB(20,20,20)
Main.Parent = Gui
Instance.new("UICorner",Main).CornerRadius = UDim.new(0,12)

-- Drag
do
    local dragging, start, pos
    Main.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            start = input.Position
            pos = Main.Position
        end
    end)
    UIS.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local delta = input.Position - start
            Main.Position = pos + UDim2.fromOffset(delta.X, delta.Y)
        end
    end)
    UIS.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
end

-- Header
local Header = Instance.new("Frame",Main)
Header.Size = UDim2.new(1,0,0,40)
Header.BackgroundTransparency = 1

local Title = Instance.new("TextLabel",Header)
Title.Size = UDim2.new(1,-80,1,0)
Title.Position = UDim2.new(0,10,0,0)
Title.Text = "Loader | discord.gg/threaded"
Title.TextColor3 = Color3.new(1,1,1)
Title.TextXAlignment = Enum.TextXAlignment.Left
Title.BackgroundTransparency = 1
Title.Font = Enum.Font.GothamBold
Title.TextSize = 14

-- Minimize
local Min = Instance.new("TextButton",Header)
Min.Size = UDim2.fromOffset(30,30)
Min.Position = UDim2.new(1,-70,0,5)
Min.Text = "-"
Min.Font = Enum.Font.GothamBold
Min.TextSize = 20
Min.BackgroundColor3 = Color3.fromRGB(30,30,30)
Min.TextColor3 = Color3.new(1,1,1)
Instance.new("UICorner",Min).CornerRadius = UDim.new(0,6)

-- Close
local Close = Instance.new("TextButton",Header)
Close.Size = UDim2.fromOffset(30,30)
Close.Position = UDim2.new(1,-35,0,5)
Close.Text = "X"
Close.Font = Enum.Font.GothamBold
Close.TextSize = 14
Close.BackgroundColor3 = Color3.fromRGB(120,30,30)
Close.TextColor3 = Color3.new(1,1,1)
Instance.new("UICorner",Close).CornerRadius = UDim.new(0,6)

-- Open Loader button
local OpenBtn = Instance.new("TextButton",Gui)
OpenBtn.Size = UDim2.fromOffset(140,35)
OpenBtn.Position = UDim2.fromScale(0.5,0.05)
OpenBtn.AnchorPoint = Vector2.new(0.5,0)
OpenBtn.Text = "Open Loader"
OpenBtn.Visible = false
OpenBtn.BackgroundColor3 = Color3.fromRGB(25,25,25)
OpenBtn.TextColor3 = Color3.new(1,1,1)
OpenBtn.Font = Enum.Font.Gotham
OpenBtn.TextSize = 14
Instance.new("UICorner",OpenBtn).CornerRadius = UDim.new(0,8)

-- Footer
local Footer = Instance.new("TextLabel",Main)
Footer.Size = UDim2.new(1,0,0,20)
Footer.Position = UDim2.new(0,0,1,-22)
Footer.Text = "By Ava"
Footer.TextColor3 = Color3.fromRGB(150,150,150)
Footer.BackgroundTransparency = 1
Footer.Font = Enum.Font.Gotham
Footer.TextSize = 12

-- Tab system
local Tabs = {}
local Pages = {}

local TabBar = Instance.new("Frame",Main)
TabBar.Size = UDim2.new(1,0,0,30)
TabBar.Position = UDim2.new(0,0,0,40)
TabBar.BackgroundTransparency = 1

local function SwitchTab(name)
    for _,v in pairs(Pages) do v.Visible = false end
    Pages[name].Visible = true
end

local function CreateTab(name)
    local Page = Instance.new("Frame",Main)
    Page.Size = UDim2.new(1,0,1,-70)
    Page.Position = UDim2.new(0,0,0,70)
    Page.Visible = false
    Page.BackgroundTransparency = 1
    Pages[name] = Page

    local Scroll = Instance.new("ScrollingFrame",Page)
    Scroll.Size = UDim2.new(1,0,1,-30) -- leave space for footer
    Scroll.BackgroundTransparency = 1
    Scroll.CanvasSize = UDim2.new(0,0,0,0)
    Scroll.ScrollBarThickness = 6
    Scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y

    local UIList = Instance.new("UIListLayout",Scroll)
    UIList.SortOrder = Enum.SortOrder.LayoutOrder
    UIList.Padding = UDim.new(0,5)

    -- Tab button
    local btn = Instance.new("TextButton",TabBar)
    btn.Size = UDim2.fromOffset(100,30)
    btn.Position = UDim2.fromOffset((#TabBar:GetChildren()-1)*105,0)
    btn.Text = name
    btn.BackgroundColor3 = Color3.fromRGB(35,35,35)
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 14
    Instance.new("UICorner",btn).CornerRadius = UDim.new(0,6)
    btn.MouseButton1Click:Connect(function() SwitchTab(name) end)

    return Page, Scroll
end

-- Create tabs
local LoadersTab, LoadersScroll = CreateTab("Loaders")
local LinksTab, LinksScroll = CreateTab("Links")
SwitchTab("Loaders")

-- Buttons functionality
Min.MouseButton1Click:Connect(function()
    Main.Visible = false
    OpenBtn.Visible = true
end)
OpenBtn.MouseButton1Click:Connect(function()
    Main.Position = UDim2.fromScale(0.5,0.5)
    Main.Visible = true
    OpenBtn.Visible = false
end)
Close.MouseButton1Click:Connect(function()
    getgenv()._LOADER_CLOSED = true
    Gui:Destroy()
end)

-- Startup popup
do
    local StartPopup = Instance.new("Frame",Gui)
    StartPopup.Size = UDim2.fromScale(0.3,0.2)
    StartPopup.Position = UDim2.fromScale(0.35,0.4)
    StartPopup.BackgroundColor3 = Color3.fromRGB(35,35,35)
    Instance.new("UICorner",StartPopup).CornerRadius = UDim.new(0,10)

    local Label = Instance.new("TextLabel",StartPopup)
    Label.Size = UDim2.new(1,0,0.6,0)
    Label.Text = "Report bugs or make suggestions at sq.ax/3P"
    Label.TextColor3 = Color3.new(1,1,1)
    Label.BackgroundTransparency = 1
    Label.TextWrapped = true
    Label.Font = Enum.Font.Gotham
    Label.TextSize = 14

    local OkBtn = Instance.new("TextButton",StartPopup)
    OkBtn.Size = UDim2.new(0.4,-5,0.3,0)
    OkBtn.Position = UDim2.new(0.3,0,0.65,0)
    OkBtn.Text = "OK"
    OkBtn.BackgroundColor3 = Color3.fromRGB(50,150,50)
    OkBtn.TextColor3 = Color3.new(1,1,1)
    Instance.new("UICorner",OkBtn).CornerRadius = UDim.new(0,6)

    OkBtn.MouseButton1Click:Connect(function()
        setclipboard("sq.ax/3P")
        StartPopup:Destroy()
    end)
end

-- Loaders tab content
local Loaders = {
    {
        PlaceId = 155615604, -- Prison Life
        Script = function()
            loadstring(game:HttpGet("https://codeberg.org/Aava/Isrsjbeiuwkow/raw/branch/main/pluh.lua"))()
        end,
        Title = "Prison Life"
    },
    {
        Title = "Silent Aim",
        Script = function()
            loadstring(game:HttpGet("https://codeberg.org/Aava/vyfhugghygg/raw/branch/main/sssuuunnn.lua"))()
        end
    }
}

for _,data in pairs(Loaders) do
    local btn = Instance.new("TextButton",LoadersScroll)
    btn.Size = UDim2.new(1,-10,0,30)
    btn.BackgroundColor3 = Color3.fromRGB(30,30,30)
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 14
    btn.Text = data.Title or ("PlaceId: "..(data.PlaceId or ""))
    Instance.new("UICorner",btn).CornerRadius = UDim.new(0,6)

    btn.MouseButton1Click:Connect(function()
        if data.PlaceId then
            if game.PlaceId == data.PlaceId then
                data.Script()
            else
                local warnLabel = Instance.new("TextLabel",LoadersScroll)
                warnLabel.Size = UDim2.new(1,0,0,25)
                warnLabel.Text = "âš  Wrong game"
                warnLabel.TextColor3 = Color3.fromRGB(255,200,0)
                warnLabel.BackgroundTransparency = 1
                warnLabel.Font = Enum.Font.GothamBold
                warnLabel.TextSize = 14
                warnLabel.LayoutOrder = #LoadersScroll:GetChildren()+1
            end
        else
            data.Script() -- universal loader
        end
    end)
end

-- Links tab content
local Links = {
    {Name="Scriptblox", URL="https://scriptblox.com/u/Aava"},
    {Name="TikTok", URL="https://www.tiktok.com/@aavauz?_r=1&_t=ZT-9388YgEd0wQ"},
    {Name="Rscript", URL="https://rscripts.net/@ava"},
    {Name="YouTube", URL="https://www.youtube.com/@Avaauz"},
    {Name="GitHub", URL="https://github.com/Avauz"}
}

for _,link in pairs(Links) do
    local btn = Instance.new("TextButton",LinksScroll)
    btn.Size = UDim2.new(1,-10,0,30)
    btn.BackgroundColor3 = Color3.fromRGB(30,30,30)
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 14
    btn.Text = link.Name
    Instance.new("UICorner",btn).CornerRadius = UDim.new(0,6)

    btn.MouseButton1Click:Connect(function()
        setclipboard(link.URL)
    end)
end

